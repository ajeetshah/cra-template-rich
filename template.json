{
  "package": {
    "dependencies": {
      "@reduxjs/toolkit": "^1.5.0",
      "@testing-library/jest-dom": "^5.11.6",
      "@testing-library/react": "^11.2.2",
      "@testing-library/user-event": "^12.6.0",
      "@types/jest": "^26.0.19",
      "@types/lodash": "^4.14.165",
      "@types/node": "^14.14.14",
      "@types/react": "^17.0.0",
      "@types/react-datepicker": "^3.1.2",
      "@types/react-dom": "^17.0.0",
      "@types/react-redux": "^7.1.12",
      "@types/react-router-dom": "^5.1.6",
      "axios": "^0.21.0",
      "bootstrap": "^4.5.3",
      "env-cmd": "^10.1.0",
      "json-server": "^0.16.3",
      "lodash": "^4.17.20",
      "react-bootstrap": "^1.5.0",
      "react-datepicker": "^3.3.0",
      "react-hook-form": "^6.13.1",
      "react-intl": "^5.10.9",
      "react-redux": "^7.2.2",
      "react-router-dom": "^5.2.0",
      "react-spinners": "^0.10.6",
      "react-toastify": "^7.0.3",
      "rimraf": "^3.0.2",
      "sass": "^1.30.0",
      "serialize-javascript": "^5.0.1",
      "source-map-explorer": "^2.5.2",
      "typescript": "^4.1.3",
      "web-vitals": "^1.0.1"
    },
    "scripts": {
      "start": "react-scripts start",
      "test": "react-scripts test --watchAll=false",
      "coverage": "react-scripts test --watchAll=false --coverage",
      "clean-deps": "rimraf node_modules/ yarn.lock",
      "eject": "react-scripts eject",
      "analyze": "source-map-explorer 'build/static/js/*.js'",
      "clean": "rimraf build/",
      "build": "react-scripts build",
      "development": "env-cmd -f envs/.env.development react-scripts build",
      "staging": "env-cmd -f envs/.env.staging react-scripts build",
      "production": "env-cmd -f envs/.env.production react-scripts build",
      "json-server": "json-server -c mock/config.json mock/index.js"
    },
    "eslintConfig": {
      "extends": [
        "react-app",
        "react-app/jest",
        "plugin:jsx-a11y/recommended"
      ],
      "plugins": [
        "jsx-a11y"
      ]
    },
    "jest": {
      "collectCoverageFrom": [
        "src/**/*.{js,jsx,ts,tsx}",
        "!<rootDir>/node_modules/",
        "!<rootDir>/src/translations/**/*",
        "!<rootDir>/src/styles/**/*",
        "!<rootDir>/src/**/*.stories.tsx",
        "!<rootDir>/src/**/*Slice.ts",
        "!<rootDir>/src/serviceWorker.js",
        "!<rootDir>/src/constants/**/*"
      ],
      "coverageThreshold": {
        "global": {
          "branches": 0,
          "functions": 0,
          "lines": 0,
          "statements": 0
        }
      },
      "coverageReporters": [
        "json",
        "lcov",
        "text",
        "clover"
      ]
    }
  }
}